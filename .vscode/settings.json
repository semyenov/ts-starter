{
  "editor.tabCompletion": "off",
  "editor.defaultFormatter": "dprint.dprint",
  "editor.formatOnSave": true,
  "editor.wrappingStrategy": "advanced",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "deepIndent",
  "where-am-i.icon": "home",
  "where-am-i.template": "{icon} {project-name}",
  "where-am-i.colorful": true,
  "where-am-i.color": "#fff",
  // "where-am-i": false,
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
  ],
  "browse-lite.startUrl": "http://localhost:3000",
  "browse-lite.chromeExecutable": "/usr/bin/chromium",
  "remote.extensionKind": {
    "GitHub.copilot": ["ui", "workspace"],
    "GitHub.copilot-labs": ["ui", "workspace"],
  },
  "explorer.fileNesting.patterns": {
    "nuxt.config.*": "*.env, .babelrc*, .codecov, .cssnanorc*, .env.*, .envrc, .htmlnanorc*, .lighthouserc.*, .mocha*, .postcssrc*, .terserrc*, api-extractor.json, ava.config.*, babel.config.*, contentlayer.config.*, cssnano.config.*, cypress.*, env.d.ts, formkit.config.*, formulate.config.*, histoire.config.*, htmlnanorc.*, jasmine.*, jest.config.*, jsconfig.*, karma*, lighthouserc.*, playwright.config.*, postcss.config.*, puppeteer.config.*, rspack.config.*, svgo.config.*, tailwind.config.*, tsconfig.*, tsdoc.*, uno.config.*, unocss.config.*, vitest.config.*, webpack.config.*, windi.config.*, i18n.config.*",
  },
  "fileNestingUpdater.autoUpdate": true,
  "fileNestingUpdater.autoUpdateInterval": 720,
  "fileNestingUpdater.promptOnAutoUpdate": true,
  "fileNestingUpdater.upstreamRepo": "antfu/vscode-file-nesting-config",
  "fileNestingUpdater.upstreamBranch": "main",
  "[vue]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[markdown]": {
    "editor.formatOnSave": false,
  },
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true,
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "npm.packageManager": "yarn",
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
}
